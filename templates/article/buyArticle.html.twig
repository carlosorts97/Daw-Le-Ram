{% extends 'base.html.twig' %}
{% block stylesheets %}
    {{ parent() }}
{% endblock %}
{% block body %}
    <div class="container margin_scroll">
        <div class="row justify-content-center">
            <div class="col-lg-6 box-form">
                <h2 class="text-center"></h2>
                <div class="row">
                    <div class="col-lg-12">
                        {{ form_row(formU.address) }}
                    </div>
                </div>
                {{form_start(form)}}
                <div class="row">
                    <div class="col-lg-12">
                        {{ form_row(form.owner) }}
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        {{ form_row(form.number) }}
                    </div>
                </div>

                <div class="row">
                    <div class="col-lg-12">
                        {{ form_row(form.cvv) }}
                    </div>
                </div>


                <div class="row">
                    <div class="col-lg-12">
                        {{ form_row(form.endDate) }}
                    </div>
                </div>


                <input type="submit" class="btn btn-navegar mt-4" value="Register">
                {{ form_errors(form) }}
                {{ form_end(form) }}
            </div>
        </div>
    </div>
{% endblock %}